{
  "name": "matafome-api",
  "version": "1.0.0",
  "description": "API desenvolvida para alimentar sistema de lanchonete online.",
  "main": "./dist/server",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prisma": "ts-node prisma/seed.ts",
    "build": "if [ -d dist ]; then rm -rf dist; fi && tsc && npx prisma migrate dev && npx prisma db seed --preview-feature && npx prisma generate",
    "start": "node ./dist/server.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "prisma:generate": "prisma generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/igor93araujo/mataFome-API.git"
  },
