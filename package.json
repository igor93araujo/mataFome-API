  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "express": "^4.18.2",
    "nodemon": "^2.0.22",
    "prisma": "^4.16.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@prisma/client": "^4.16.0",
    "cors": "^2.8.5"
  }
